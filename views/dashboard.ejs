<div class="container">
    <h2> Users</h2>
    <table class="table">
        <thead>
          <tr>
            <th scope="col"></th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>  
          </tr>
        </thead>
        <tbody>
        <% users.forEach(function(user){ %>
          <tr class="table-success">
                <th scope="row"><%=count++%></th>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <hr>
            <% }) %> 
          </tr>  
        </tbody>
      </table> 
   
</div>



