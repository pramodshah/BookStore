<%- include('./partials/navbarLogout') %>
</br>
</br>

<div class="container">
  <div class="row">
      <h1>Users</h1>
   <!-- <table class="table table-striped table-primary"> -->
   <table class="table">
  
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <%if (users.length >0){ %>
      <% users.forEach(function(user){ %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        
        <td>
          <a class="btn btn-success btn-xs" href="/add/<%= user._id %>"><i class="fa fa-plus"></i></a>
          <a class="btn btn-primary btn-xs" href="/edit/<%= user._id %>"><i class="fa fa-edit"></i></a>
          <a class="btn btn-danger btn-xs" href="/delete/<%= user._id %>"><i class="fa fa-trash"></i></a>

        </td>
      </tr>
      <% })}else{ %>
        <tr>
            <td colspan="6">No Record Found</td>
            
          </tr>
        <% } %>
    </tbody>
  </table>
</div>
</div>




